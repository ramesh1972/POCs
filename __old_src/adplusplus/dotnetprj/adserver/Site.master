                    <tr valign=middle align=center class=siteButton height=34px><td><a href=Default.aspx>Home</a></td></tr>
                    <tr valign=middle align=center class=siteButton height=34px><td><a href=About.aspx>About Us</a></td></tr>
                    <tr valign=middle align=center class=siteButton height=34px><td><a href=Contact.aspx>Contact Us</a></td></tr>
                </table>
            </td>
        </tr>
    </table>
    <table width=100% height=100% border=0>
        <tr valign=top height=100%>
            <td align=left width=70%><asp:ContentPlaceHolder ID="mainContent" runat=server></asp:ContentPlaceHolder></td>
            <td align=left><asp:ContentPlaceHolder ID="mainStats" runat=server></asp:ContentPlaceHolder></td>
        </tr>
    </table>
